<jaxb:bindings
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"               
    xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
    
    version="2.1">
    <!--
      Note: the above should really be jaxb:version="2.1", but a bug in jdk6.0_18 prevents this usage.  The workaround is to
      simply use 'version' instead of 'jaxb:version'.  
      see http://forums.java.net/jive/thread.jspa?threadID=77253&tstart=0 and https://jaxp.dev.java.net/issues/show_bug.cgi?id=63
    -->
    
    
    <jaxb:bindings schemaLocation="wsdl/sawservices.xsd" node="/xsd:schema">
    
      <jaxb:schemaBindings>
        <jaxb:package name="com.siebel.analytics.web.soap.v5.model"/>
      </jaxb:schemaBindings>
    
    </jaxb:bindings>
</jaxb:bindings>